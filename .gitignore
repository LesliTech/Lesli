
.byebug_history

# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
npm-debug.log

# Ignore auto-generated documentation
/doc
/.yardoc

# Ignore uploaded files in development
/storage/*
!/storage/.keep
/public/dev-deploy-log.txt
/public/assets
/node_modules
/yarn-error.log

/engines/*
!/engines/.gitkeep

# Ignore master key for decrypting credentials and more.
/config/master.key
/config/credentials/*

# Production encrypted credentials shoul be included always
!/config/credentials/production.yml.enc

*.pem
