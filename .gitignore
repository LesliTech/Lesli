# Ignore bundler config.
.bundle
.byebug_history
.DS_store
Gemfile.lock
package-lock.json


# Ignore all logfiles and tempfiles.
*.dump 
/log/*
/tmp/*
/public/tmp/*
!/log/.keep
!/tmp/.keep
!/public/tmp/.gitkeep


# Ignore auto-generated documentation
/doc
/.yardoc


# Ignore auto-generated DB schema
/db/schema.rb


# Ignore stored files in this folder
/storage/*
!/storage/.keep
!/storage/keep/
!/storage/templates/


/public/tmp
/public/assets
/public/javascripts/translations.js
/node_modules


/engines/*
!/engines/.gitkeep


# Ignore master key for decrypting credentials and more.
/config/locales
/config/master.key
/config/credentials
/config/credentials/development.key
/config/credentials/production.key
/config/credentials/test.key
!/config/credentials/.keep


*.pem
